{{#each this}}
<h1 class="card-title">{{name}}</h1>
<div class="card">
<div class="card-body">
    <p class="card-text">Capital:{{capital}}</p>
    <p class="card-text">Population:{{population}}</p>
    <p class="card-text">Languages:</p>
    <ul class="list-group">
        {{#each languages}}
        <li class="list-item">{{name}}</li>
        {{/each}}
    </ul>
</div>
<div class="card-img">
    <img src="{{flag}}" alt="{{name}}" width="600" height="400">
</div>
</div>
{{/each}}